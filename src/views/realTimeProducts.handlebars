<div class="encabezado">
   <h1>Catalogo de productos</h1> 
</div>
<form id="agregarProducto" >
        <input type="text" id='titleInput' name="title" placeholder="Nombre" required>
        <input type="text" id='descriptionInput' name="description" placeholder="Descripcion" required>
        <input type="number" id='priceInput' name="price" placeholder="Precio" required>
        <input type="text" id='statusInput' name="status" placeholder="Status" required>
        <input type="text" id='categoryInput' name="category" placeholder="Categoria" required>
        <input type="text" id='thumbnailsInput' name="thumbnails" placeholder="Img" required>
        <input type="text" id='codeInput' name="code" placeholder="Codigo" required>
        <input type="number" id="stockInput" name="stock" placeholder="Stock" required>
        <input type="submit" value="enviar">
      </form>
<div id="contenedorCard" class="container">
<section id="cardSocket" class="card">
</section>
</div>
