<div class="encabezado">
   <h1>Catalogo de productos</h1> 
</div>
<div class="container">
{{#each products}}
<section class="card">
<img src="../img/Star_Wars_Logo.svg.png" alt="Imagen de prueba" class="card-img">
      <h2>{{title}}</h2>
      <p><span>Categoria</span>: {{category}}</p>
      <article class="card-price">
        <p><span>Precio</span>: {{price}}</p>
        <p><span>Stock</span>: {{stock}}</p>   
      </article>
      <p><span>Cod. producto</span>: {{code}}</p>
      <button class="card-boton-borrar" id="deleteProduct${product.id}">Eliminar Producto</button>
</section>
{{/each}}
</div>
